@model IEnumerable<ProjectsManagementSystem.Models.Users>

@{
    ViewBag.Title = "EditMember";
}

<h2>Add Member</h2>


<table class="table">
    <tr>
        <th>
            Developer Name
        </th>
   
        <th>
            Email
        </th>
    
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem =>item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
     
        <td>
            @Html.ActionLink("Add", "AddMember", new { id=item.userID }, new { @class = "btn btn-success" })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Back to Profile", "Index",null, new { @class = "btn btn-info" })
</div>