@model IEnumerable<ProjectsManagementSystem.Models.User_Projects>
@{
    ViewBag.Title = "Evaluate_Developer";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<h2>Evaluate_Developer</h2>
<table class="table table-striped table-bordered table-hover">
    <tr>

        <th>
            Developer name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Users.name
            </td>
            <td >
                @Html.ActionLink("Evaluation Page", "confirmEvaluate_Developer", new { id =item.myUser_id },new { @class = "btn btn-info" })
              </td>
        </tr>
    }
    
</table>
}
        <div>
            @Html.ActionLink("Back to List", "Index",null, new { @class = "btn btn-success" })
        </div>
